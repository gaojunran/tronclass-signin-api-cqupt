{
  "imports": {
    "@prisma/adapter-pg": "npm:@prisma/adapter-pg@^7.0.1",
    "dotenv": "npm:dotenv@^17.2.3",
    "hono": "jsr:@hono/hono@^4.10.5",
    "@hono/hono/cors": "jsr:@hono/hono@^4.10.5/cors",
    "@prisma/client": "npm:@prisma/client@^7.0.1",
    "prisma": "npm:prisma@^7.0.1",
    "uuid": "jsr:@std/uuid@^1.0.7"
  },
  "tasks": {
    "start": "deno run -A main.ts",
    "db:generate": "deno run -A --allow-scripts --unstable npm:prisma generate",
    "db:push": "deno run -A --unstable npm:prisma db push",
    "db:studio": "deno run -A --unstable npm:prisma studio"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "nodeModulesDir": "auto"
}
